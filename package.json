{
  "name": "pomofy-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "18.12.1"
  },
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "check:format": "prettier --check .",
    "check:lint": "eslint . --ext .js,.ts --color",
    "check:spelling": "cspell \"{README.mdx,TODO.md,.github/*.md,src/**/*.ts}\"",
    "check:staged": "lint-staged",
    "check:types": "tsc --pretty --noEmit",
    "commit": "cz",
    "fix:format": "prettier --check --write --ignore-unknown .",
    "fix:lint": "eslint . --ext .js,.ts --color --fix",
    "prebuild": "rimraf dist",
    "prepare": "husky install",
    "prepare-release": "run-s reset-hard version",
    "prisma:generate": "prisma generate",
    "prisma:generate:watch": "prisma generate --watch",
    "prisma:migrate": "prisma migrate dev --skip-seed",
    "prisma:seed": "prisma db seed",
    "prisma:reset": "prisma migrate reset",
    "prisma:studio": "prisma studio",
    "prisma:format": "prisma format",
    "reset-hard": "git clean -dfx && git reset --hard && npm install",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest --colors --no-cache --passWithNoTests --detectOpenHandles --runInBand",
    "test:coverage": "npm run test -- --coverage --silent",
    "test:sonar": "node sonar-project.js",
    "test:watch": "npm run test -- --watch --verbose false",
    "version": "standard-version -t",
    "update-deps": "npm-check-updates -u"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@nestjs/common": "^9.2.1",
